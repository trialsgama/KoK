<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="reyes.de.rey.app_nativa_am">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.Register"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.Login"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
             </intent-filter>
         </activity>
        <activity
            android:name=".Activities.menuusuarioListActivity"
            android:label="@string/title_menuusuario_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Activities.menuusuarioDetailActivity"
            android:label="@string/title_menuusuario_detail"
            android:parentActivityName=".Activities.menuusuarioListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="reyes.de.rey.app_nativa_am.Activities.menuusuarioListActivity" />
        </activity>

    </application>
</manifest>